<body>

<!-- Yellow UA msg for anything older than IE9. Please remove me post end-Aug demo. -->
<!-- Compatibility message --><!--[if lt IE 9]><p class="ua-notification">GOV.AU works better on a newer web browser (the software you use to access the internet). Weâ€™re improving how it works in your browser.</p><![endif]-->

<nav class="skip-to">
  <a href="#content">Skip to main content</a>
  <a href="#nav">Skip to section navigation</a>
</nav>

<header role="banner">
  <section class="govau--header">
    <div class="wrapper">
      <div class="govau--logo">
        <a href="/" class="logo">gov.au ui-kit</a> <span class="badge--default">draft</span>
      </div>
      <div class="feedback">
        <a href="https://github.com/AusDTO/gov-au-ui-kit/issues/new" class="button--feedback" role="button">Give feedback</a>
      </div>
    </div>
  </section>
  {{# if homepage}}
  <section class="hero">
    <div class="wrapper">
      <span class="site-title">{{options.title}}</span>
      <p class="tagline">A living style guide of the UI-Kit <abbr title="Sassy CSS">SCSS</abbr> framework, built from the source, using <a href="http://warpspire.com/kss/" rel="external"><abbr title="Knyle Style Sheets">KSS</abbr></a>.</p>
      <p>Documents UI-Kit release: <code>v{{package "version"}}</code></p>
      <p>Last updated: <time datetime="{{moment "" ""}}">{{moment "" "DD MMMM Y"}}</time></p>
    </div>
  </section>
  {{/if}}
</header>

<main role="main">
  <aside class="sidebar" id="nav">
    <nav class="local-nav" aria-label="main navigation">
      <ul>
        <li>
          <a href="index.html" class="is-active">Getting started</a>
          <ul>
            {{#each menu}}
              <li>
                <a href="section-{{referenceURI}}.html" {{#if isActive}}class="is-active is-current"{{/if}}>{{header}}</a>
              </li>
            {{/each}}
          </ul>
        </li>
      </ul>
    </nav>
  </aside>

  <article id="content" class="content-main">

    {{#if homepage}}
      <div id="guide-0" class="guide-section guide-section--depth-0 kss-overview">
        {{{homepage}}}
      </div>
    {{else}}

    <header class="guide-section-header">
      <h1>{{sections.0.header}}</h1>

      <nav class="index-links">
        <h2>In this section</h2>
        <ul>
          {{#each sections}}
            {{#if_equal depth 2}}
            <li><a href="#guide-{{referenceURI}}">{{header}}</a></li>
            {{/if_equal}}
          {{/each}}
        </ul>
      </nav>
    </header>

      {{#each sections}}
        <{{#if @first}}div{{else}}section{{/if}} id="guide-{{referenceURI}}"
                                                 class="guide-section guide-section--depth-{{depth}}">

          <div>
            {{#unless @first}}
            <h{{depth}} id="#guide-{{referenceURI}}">
              {{header}}
            </h{{depth}}>
            {{/unless}}

            {{#if description}}
              <div class="kss-description">
                {{{description}}}
              </div>
            {{/if}}

            {{#if parameters}}
              <div class="kss-parameters__title">Parameters:</div>
              <ul class="kss-parameters">
                {{#each parameters}}
                  <li class="kss-parameters__item">
                    <div class="kss-parameters__name"><code>{{name}}</code></div>
                    <div class="kss-parameters__description">
                      {{{description}}}
                      {{#if defaultValue}}
                        <div class="kss-parameters__default-value">
                          Defaults to: <code>{{defaultValue}}</code>
                        </div>
                      {{/if}}
                    </div>
                  </li>
                {{/each}}
              </ul>
            {{/if}}
          </div>

          {{#if example}}
            <div class="kss-modifier__wrapper">
              <h3>Example{{#if modifiers}}s{{/if}}</h3>

              {{#if modifiers}}
                <div class="kss-modifier__default-name">
                  Default styling
                </div>
              {{/if}}

              <div class="guide-example">
                {{{example}}}
              </div>

              {{#each modifiers}}
                <div class="kss-modifier__name">
                  {{name}}
                </div>
                <div class="kss-modifier__description">
                  {{{description}}}
                </div>
                <div class="example">
                  {{{markup}}}
                </div>
              {{/each}}
            </div>
            {{#if example}}
              <pre class="prettyprint lang-html"><code data-language="html">{{markup}}</code></pre>
            {{/if}}
          {{/if}}

          {{#if sourceFile.name}}
            <div class="kss-source">
              Source on GitHub: <a href="https://github.com/AusDTO/gov-au-ui-kit/tree/master/assets/sass/{{sourceFile.name}}#L{{sourceFile.line}}" rel="external"><code>{{sourceFile.name}}</code></a>, line {{sourceFile.line}}
            </div>
          {{/if}}

        </{{#if @first}}div{{else}}section{{/if}}>
      {{/each}}
    {{/if}}
  </article>
</main>

<!-- SCRIPTS -->
<script src="kss-assets/kss.js"></script>
{{{scripts}}}

<footer role="contentinfo">
  <div class="wrapper">
    <div class="feedback">
      <a href="https://github.com/AusDTO/gov-au-ui-kit/issues/new" class="button--feedback" role="button">Give feedback</a>
    </div>
    <section>
      <div class="footer-logo">
        <img alt="Australian Government Coat of Arms" src="/latest/img/coat-of-arms.png">
      </div>
      <div class="footer-links">
        <nav>
          <ul>
            <li><a href="https://www.dto.gov.au/privacy-statement/" rel="external">Privacy Statement</a></li>
          </ul>
        </nav>
        <p>&copy; Commonwealth of Australia <br>
        <a href="https://github.com/AusDTO/gov-au-ui-kit/blob/master/LICENSE.md" rel="external license">Licensed under the MIT License.</a></p>
      </div>
    </section>
  </div>
</footer>

